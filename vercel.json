{
  "version": 2,
  "name": "ai-scratch",
  "builds": [
    {
      "src": "/content/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/content/index.html"
    },
    {
      "src": "/([^.]+)\\.(html|css|js)",
      "dest": "/content/$1.$2"
    }
  ]
}
