{
  "version": 2,
  "name": "ai-scratch",
  "builds": [
    {
      "src": "/content/*",
      "use": "@vercel/static"
    },
    {
      "src": "/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "/api/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "/api/*.txt",
      "use": "@vercel/static"
    },
    {
      "src": "/api/*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/content/index.html"
    },
    {
      "src": "/api/([^.]+)",
      "dest": "/api/$1"
    },
    {
      "src": "/([^.]+)\\.(html|css|ts|tsx|text|json)",
      "dest": "/content/$1.$2"
    },
    {
      "src": "/([^.]+)\\.(png|jpg|svg|gif)",
      "dest": "/images/$1.$2"
    },
    {
      "src": "/([^.]+)\\.(zip|type|sb|sb2|sb3|scratch|pmp|dbp)",
      "dest": "/archives/$1.$2"
    }
  ]
}
